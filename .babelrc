{
  "presets": [
    [
      "@babel/preset-env",
      {
        "debug": true,
        "targets": {
          "node": "current",
          "browsers": [
            "last 1 versions",
            "> 1%",
            "ie >= 11"
          ]
        },
        "useBuiltIns": "usage",
        "corejs": 3
      }
    ]
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "alias": {
          "@Config": "./config",
          "@Server": "./src/server",
          "@Admin": "./src/client/js/admin",
          "@Public": "./src/client/js/public"
        }
      }
    ]
  ]
}