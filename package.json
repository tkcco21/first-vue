{
  "name": "first-vue",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "engines": {
    "node": "10.18.1",
    "npm": "6.13.6"
  },
  "scripts": {
    "rm:dist": "rimraf ./dist",
    "server": "nodemon mock/server.js --watch ./mock",
    "dev": "NODE_ENV=development webpack-dev-server",
    "start": "npm run rm:dist && npm run dev",
    "build": "npm run rm:dist && NODE_ENV=production webpack",
    "stats": "webpack --profile --json > dist/stats.json",
    "analyze": "webpack-bundle-analyzer dist/stats.json --port 8765"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,vue}": "eslint"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "IE >= 11"
  ],
  "dependencies": {
    "@babel/register": "^7.9.0",
    "axios": "^0.19.0",
    "config": "^3.1.0",
    "core-js": "^3.0.1",
    "dotenv": "^8.0.0",
    "helmet": "^3.21.2",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "vee-validate": "^2.2.7",
    "vue": "^2.6.11",
    "vue-lazyload": "^1.3.2",
    "vue-router": "^3.0.3",
    "vuetify": "^1.5.14",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@babel/preset-env": "7.9.0",
    "@mdi/font": "4.9.95",
    "autoprefixer": "9.7.5",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "babel-plugin-module-resolver": "4.0.0",
    "browserslist": "4.11.0",
    "compression-webpack-plugin": "3.1.0",
    "copy-webpack-plugin": "5.1.1",
    "css-loader": "3.4.2",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.1.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-loader": "3.0.3",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-vue": "6.2.2",
    "file-loader": "5.1.0",
    "html-webpack-plugin": "3.2.0",
    "husky": "4.2.3",
    "json-server": "^0.16.1",
    "lint-staged": "10.0.9",
    "mini-css-extract-plugin": "0.9.0",
    "nodemon": "^2.0.2",
    "postcss-color-function": "4.1.0",
    "postcss-custom-media": "7.0.8",
    "postcss-custom-properties": "9.1.1",
    "postcss-import": "12.0.1",
    "postcss-loader": "3.0.0",
    "postcss-mixins": "6.2.3",
    "postcss-nested": "4.2.1",
    "rimraf": "3.0.2",
    "stats-webpack-plugin": "0.7.0",
    "vue-loader": "15.9.1",
    "vue-style-loader": "4.1.2",
    "vue-template-compiler": "2.6.11",
    "webpack": "4.42.1",
    "webpack-bundle-analyzer": "3.6.1",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.10.3",
    "webpack-notifier": "1.8.0"
  }
