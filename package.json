{
  "name": "first-vue",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "engines": {
    "node": "10.15.0",
    "npm": "6.9.0"
  },
  "scripts": {
    "migrate:dev": "sequelize db:migrate",
    "migrate:prd": "NODE_ENV=production sequelize db:migrate",
    "rm:dist": "rimraf ./dist/js ./dist/css",
    "dev:server": "DEBUG=first-vue:* nodemon -L ./src/server/index.js",
    "dev": "npm run rm:dist && npm run dev:server",
    "build": "npm run rm:dist && NODE_ENV=production webpack",
    "eslint:server": "eslint ./src/server/**",
    "start": "pm2-runtime start ecosystem.config.js --env production",
    "stats": "webpack --profile --json > dist/stats.json",
    "analyze": "webpack-bundle-analyzer dist/stats.json --port 8765"
  },
  "browserslist": [
    "last 1 versions",
    "> 1%",
    "IE >= 11"
  ],
  "dependencies": {
    "@babel/register": "^7.4.0",
    "axios": "^0.19.0",
    "config": "^3.1.0",
    "cookie-parser": "^1.4.4",
    "core-js": "^3.0.1",
    "dotenv": "^8.0.0",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "helmet": "^3.18.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "mysql2": "^1.6.5",
    "pm2": "^3.5.0",
    "sanitize-html": "^1.20.1",
    "sequelize": "^5.8.12",
    "sequelize-cli": "^5.5.0",
    "vee-validate": "^2.2.7",
    "vue": "^2.6.6",
    "vue-lazyload": "^1.3.2",
    "vue-router": "^3.0.3",
    "vuetify": "^1.5.14",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@mdi/font": "^3.6.95",
    "autoprefixer": "^9.4.7",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-module-resolver": "^3.2.0",
    "browserslist": "^4.6.6",
    "compression-webpack-plugin": "^3.0.0",
    "css-loader": "^2.1.0",
    "eslint": "^5.13.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-vue": "^5.1.0",
    "file-loader": "^3.0.1",
    "hard-source-webpack-plugin": "^0.13.1",
    "mini-css-extract-plugin": "^0.6.0",
    "nodemon": "^1.18.11",
    "postcss-color-function": "^4.1.0",
    "postcss-custom-media": "^7.0.8",
    "postcss-custom-properties": "^8.0.10",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-mixins": "^6.2.1",
    "postcss-nested": "^4.1.2",
    "rimraf": "^2.6.3",
    "stats-webpack-plugin": "^0.7.0",
    "style-loader": "^0.23.1",
    "vue-loader": "^15.6.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.6",
    "webpack": "^4.34.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.4",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-hot-middleware": "^2.25.0",
